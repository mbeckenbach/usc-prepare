{
  "projectType": "application",
  "root": "apps/notebook",
  "sourceRoot": "apps/notebook/src",
  "prefix": "nb",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/notebook",
        "index": "apps/notebook/src/index.html",
        "main": "apps/notebook/src/main.ts",
        "polyfills": "apps/notebook/src/polyfills.ts",
        "tsConfig": "apps/notebook/tsconfig.app.json",
        "inlineStyleLanguage": "sass",
        "assets": ["apps/notebook/src/favicon.ico", "apps/notebook/src/assets"],
        "styles": ["apps/notebook/src/styles.sass"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/notebook/src/environments/environment.ts",
              "with": "apps/notebook/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "notebook:build:production"
        },
        "development": {
          "browserTarget": "notebook:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4302
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "notebook:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/notebook/src/**/*.ts",
          "apps/notebook/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/notebook"
      ],
      "options": {
        "jestConfig": "apps/notebook/jest.config.js",
        "passWithNoTests": true
      }
    },
    "food": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "echo \"pizza \uD83C\uDF55\""
      },
      "configurations": {
        "hamburger": {
          "command": "echo \"hamburger \uD83C\uDF54\""
        }
      }
    },
    "party": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "echo \"party \uD83C\uDF89\","
      }
    },
    "food-party": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "echo \"pizza \uD83C\uDF55\"",
          "echo \"party \uD83C\uDF89\""
        ],
        "parallel": false
      },
      "configurations": {
        "hamburger": {
          "commands": [
            "echo \"hamburger \uD83C\uDF54\"",
            "echo \"party \uD83C\uDF89\""
          ]
        }
      }
    },
    "food-party-cheese": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "echo \"cheese \uD83E\uDDC0\""
      },
      "configurations": {
        "hamburger": {},
        "donut": {}
      },
      "dependsOn": [
        {
          "target": "food",
          "projects": "self"
        },
        {
          "target": "party",
          "projects": "self"
        }
      ]
    }
  },
  "tags": []
}
